# Better Auth (Frontend Client)
# URL of your backend auth server
NEXT_PUBLIC_BETTER_AUTH_URL="http://localhost:3001"

# App URL (this frontend)
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Optional: OAuth Providers (if handled on frontend)
# NEXT_PUBLIC_GOOGLE_CLIENT_ID=""
# NEXT_PUBLIC_GITHUB_CLIENT_ID=""

# Mock Authentication (Development Only)
# Set to "true" to enable mock auth, or leave unset to auto-enable in development
# ENABLE_MOCK_AUTH="true"

# Mock User Credentials (used when mock auth is enabled)
MOCK_USER_EMAIL="demo@hypedrive.test"
MOCK_USER_PASSWORD="Demo@123456"

# MSW API Mocking (Development Only)
# Set to "enabled" to use MSW for mocking all API routes
# This allows the app to run without a backend server
# NEXT_PUBLIC_API_MOCKING="enabled"
